package DataConvert.Objects;

import java.util.Date;

public class GenbankBean{
	//Properties of this Genbank bean
	private String acc;				
	private String gi;
	private Date genDate;
	
	/**
	 * The Empty Constructor for this class
	 */
	public GenbankBean(){}
	
	/**
	 * This constructor takes in a name and a sequence to
	 * easily create this class with the basic variables.
	 *
	 * @param String Name String of the name of the taxa
	 * @param String Seq String value of the sequence of the taxa
	 */
	public GenbankBean(String Acc, String Gi, Date GenDate)
	{
		acc = Acc;
		gi = Gi;
		genDate = GenDate;
	}//Constructor
	
	/**
	 * Set the Accession to that passed in
	 * 
	 */
	public void setAcc(String Acc)
	{
		acc = Acc;
	}//setAcc
	
	/**
	 * Returns the String of the Accession
	 */
	public String getAcc()
	{
		return (acc);
	}//getSequence
	
	/**
	 * Set the GI to that passed in
	 * 
	 */
	public void setGI(String Gi)
	{
		gi = Gi;
	}//setName
	
	/**
	 * Returns the String of the GI
	 */
	public String getGI()
	{
		return (gi);
	}//getName
	
	/**
	 * Returns the Date
	 */
	public Date getDate()
	{
		return (genDate);
	}//getName
	
}//GenbankBean
