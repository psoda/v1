/**
 *	EvpthwyBean.class is an instantiated data object that
 *	contains all of the values of a given evaluated property
 *	
 *	@author	Joshua Sailsbery
 *	@version	1.0
 *	@since	1.0
 */
package treesaap.Evpthwy;

class EvpthwyBean
{
	//Bean Values
	private int catNum;
	private int conf;
	private double value;
	private String name;

	
	/**
	 *  Set method - sets value to new double
	 *	@param double Value of new value
	 */
	public void setValue(double Value)
	{
		value = Value;
	}
	
	/**
	 *  Get method - returns double value 
	 */
	public double getValue()
	{
		return value;
	}
	
	/**
	 *  Set method - sets conf to new int
	 *	@param int Conf of new conf
	 */
	public void setConf(int Conf)
	{
		conf = Conf;
	}
	
	/**
	 *  Get method - returns int conf 
	 */
	public int getConf()
	{
		return conf;
	}
	
	/**
	 *  Set method - sets name to new String
	 *	@param String Name of new name
	 */
	public void setName(String Name)
	{
		name = Name;
	}
	
	/**
	 *  Get method - returns String name 
	 */
	public String getName()
	{
		return name;
	}
	
	/**
	 *  Set method - sets catNum to new int
	 *	@param int CatNum of new catNum
	 */
	public void setCatNum(int CatNum)
	{
		catNum = CatNum;
	}
	
	/**
	 *  Get method - returns int catNum 
	 */
	public int getCatNum()
	{
		return catNum;
	}
	
}//EvpthwyBean