#NEXUS


begin data;
	dimensions ntax=6 nchar=10;
	format datatype=dna gap=- matchchar=. ;
matrix

Nicotiana  GTTGACAATT
Galphimia  .A.....G..
Oenothera  ..C.....CC
Victoria   .A...T....
Cypirapea  ..GTG.....
Barclaya   ...A..G...

;
end;
BEGIN PAUP;
	set nowarnreset 
	autoclose;
	set 
		maxtrees = 1000
		increase = auto
		autoinc = 100;
	hsearch 
		start = stepwise
		swap = none
		addseq = random
		nreps = 1
		hold = 10;
		 
	savetrees file = stepsmall.tre;
[This block will estimate a maximum parsimony tree assuming equal weights and save the results of the search to a log file.]
  quit warntsave=no;
end;
