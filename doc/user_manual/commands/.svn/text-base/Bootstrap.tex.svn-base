\section{bootstrap}

bootstrap instruction

\subsection{Options}
\begin{description}
\item[bseed] = clock
\item[blocksize] = 10
\item[nreps] = 100
\item[search] = heuristic
\item[conlevel] = 50
\item[keepall] = no
\item[wts] = simple
\item[nchar] = current
\end{description}

\subsection{Option Descriptions}
\begin{description}

\item[bseed] Random number generator seed. Default is seed from clock
\item[blocksize] Bootstrap block size for dependent columns
\item[nreps]
\item[search]
	\begin{description}
	\item[heuristic]
	\item[nj] Use Neighbor-Joining to build the tree
	\item[upgma]
	\end{description}
\item[conlevel] Confidence Level for boostrap consensus
\item[keepall] Keep all trees?
	\begin{description}
	\item[no]
	\item[yes] 
	\end{description}
\item[wts]
	\begin{description}
	\item[simple]
	\item[ignore]
	\item[repeatcnt]
	\item[proportional] 
	\end{description}
\item[nchar]

\end{description}
