\section{Set}

Set Instruction

\subsection{Options}
\begin{description}
\item[SET\_NAME\_PARAM] = " "
\item[SET\_VALUE\_PARAM] = " "
\item[SET\_COMMAND\_PARAM] = SET\_FOR\_ALL\_COMMANDS
\end{description}

\subsection{Option Descriptions}
\begin{description}
\item[SET\_NAME\_PARAM] = " "
	\begin{description}
	\item[" "] = the name of the default parameter to be set
	\end{description}
\item[SET\_VALUE\_PARAM] = " "
	\begin{description}
	\item[" "] = The value to which the parameter should be set
	\end{description}
\item[SET\_COMMAND\_PARAM] = SET\_FOR\_ALL\_COMMANDS
	\begin{description}
	\item[SET\_FOR\_ALL\_COMMANDS] = The default value for the given parameter will be set on this command (by default, the set is done on all commands that have the given parameter
	\end{description}
\end{description}